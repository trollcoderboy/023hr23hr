    part.Position = TargetCF -- teleport
    character.HumanoidRootPart.CFrame = part.CFrame
    character.HumanoidRootPart.CFrame = part.CFrame
    character.PrimaryPart = character.HumanoidRootPart


    part:Destroy()
end)
